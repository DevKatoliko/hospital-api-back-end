package model.entities;

public class Hospital {
	private Long id;
	private String name;
	private Address address;
	private String telephone;
	private double consultationPrice;
	private double hospitalizationPrice;
	private double hospitalTotalCash;
	
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Address getAddress() {
		return address;
	}
	public void setAddress(Address address) {
		this.address = address;
	}
	public String getTelephone() {
		return telephone;
	}
	public void setTelephone(String telephone) {
		this.telephone = telephone;
	}
	public double getConsultationPrice() {
		return consultationPrice;
	}
	public void setConsultationPrice(double consultationPrice) {
		this.consultationPrice = consultationPrice;
	}
	public double getHospitalizationPrice() {
		return hospitalizationPrice;
	}
	public void setHospitalizationPrice(double hospitalizationPrice) {
		this.hospitalizationPrice = hospitalizationPrice;
	}
	public double getHospitalTotalCash() {
		return hospitalTotalCash;
	}
	public void setHospitalTotalCash(double hospitalTotalCash) {
		this.hospitalTotalCash = hospitalTotalCash;
	}
	
	
}
