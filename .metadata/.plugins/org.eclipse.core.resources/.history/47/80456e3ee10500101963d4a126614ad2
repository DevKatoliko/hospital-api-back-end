package model.entities;

import java.time.LocalDate;
import java.util.List;

import model.enums.Gender;

public class Patient extends Person{
	private List<Doctor> doctors;
	private List<MedicalReport> medicalReports;
	
	protected  Patient() {
		
	}
	public Patient(List<Doctor> doctors, List<MedicalReport> medicalReports,String name,String lastName,Gender gender,String cpf,LocalDate birthDate,String telephoneNumber,
			String cellphoneNumber,String email,	Address address, Hospital hospital) {
		super();
		this.doctors = doctors;
		this.medicalReports = medicalReports;
	}
	public List<Doctor> getDoctors() {
		return doctors;
	}
	public void setDoctors(List<Doctor> doctors) {
		this.doctors = doctors;
	}
	public List<MedicalReport> getMedicalReports() {
		return medicalReports;
	}
	public void setMedicalReports(List<MedicalReport> medicalReports) {
		this.medicalReports = medicalReports;
	}
	
}
